{
  "name": "vmdevelopment/laravel-tap-payments",
  "description": "Tap Payment REST API package for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Vanand Mkrtchyan",
      "email": "vanand@vm-development.com",
      "homepage": "http://www.vm-development.com/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/support": "^8.0|^9.0",
    "guzzlehttp/guzzle": ">=6.0"
