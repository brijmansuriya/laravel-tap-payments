{
  "name": "brijmansuriya/laravel-tap-payments",
  "description": "Tap Payment REST API package for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Mansuriya brij",
      "email": "mansuriyabri@gmail.com",
      "homepage": "https://www.web-beast.com/"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/support": "^11.0|^12.0",
    "guzzlehttp/guzzle": "^6.0|^7.0|^8.0"
  },
  "version": "1.4.0",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/brijmansuriya/laravel-tap-payments.git",
      "branch": "v1"
    }
  ],
  "autoload": {
    "psr-4": {
      "BMansuriya\\TapPayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BMansuriya\\TapPayment\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BMansuriya\\TapPayment\\TapPaymentServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/framework": "11.0"
  }
}
